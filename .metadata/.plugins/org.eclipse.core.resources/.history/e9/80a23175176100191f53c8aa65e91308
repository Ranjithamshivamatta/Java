package com.java.arrayDemo;

public class TwoDArray {

	public static void main(String[] args) {

		for(int i = 0 ; i < 3 ; i++){
			for(int j = 0 ; j < 3 ; j++){
				System.out.print(i+" "+j);
			}
			System.out.println();

		}

		//		for(int i = 1; i<3 ; i++){
