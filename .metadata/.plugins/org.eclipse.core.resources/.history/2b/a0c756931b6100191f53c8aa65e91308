package com.java.arrayDemo;
class A{
	public void add(int a){
		System.out.println("In A"+ a);
	}
}
class B extends A{
	public void add(int a){
		System.out.println("In B"+a);
	}
}



public class TwoDArray {

	public static void main(String[] args)  {
		B b = new B();
		b.add(10);
		
		                  
    
	
	}

}